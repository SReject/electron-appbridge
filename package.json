{
    "name": "electron-appbridge",
    "version": "0.0.2",
    "license": "ISC",
    "scripts": {
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "build": "node build.mjs",
        "declarations": "tsc --emitDeclarationOnly"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./index.mjs",
            "require": "./index.cjs",
            "default": "./index.cjs"
        },
        "./main": {
            "types": "./index.d.ts",
            "import": "./index.mjs",
            "require": "./index.cjs",
            "default": "./index.cjs"
        },
        "./bridge": {
            "types": "./bridge.d.ts",
            "import": "./bridge.mjs",
            "require": "./bridge.cjs",
            "default": "./bridge.cjs"
        },
        "./renderer": {
            "types": "./renderer.d.ts",
            "import": "./renderer.mjs",
            "require": "./renderer.cjs",
            "default": "./renderer.cjs"
        },
        "./preload": {
            "types": "./preload.d.ts",
            "import": "./preload.mjs",
            "require": "./preload.cjs",
            "default": "./reload.cjs"
        }
    },
    "devDependencies": {
        "@types/node": "^18.14.2",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "esbuild": "^0.17.11",
        "eslint": "^8.35.0",
        "typescript": "^4.9.5"
    },
    "peerDependencies": {
        "electron": ">=12.0.0"
    },
    "engines": {
        "node": ">=14.16.0"
    },
    "volta": {
        "node": "18.14.2"
    },
    "homepage": "https://github.com/SReject/electron-appbridge",
    "repository": "github:SReject/electron-appbridge",
    "bugs": {
        "url": "https://github.com/SReject/electron-appbridge/issues"
    }
}
